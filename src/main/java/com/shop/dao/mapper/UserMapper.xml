<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.dao.mapper.UserMapper">

	<insert id="regist">
		insert into
		shop_user(id,username,password,e_mail,phone)
		values(seq_shop_use.nextval,#{username},#{password},#{eMail},#{phone})
	</insert>
	
	<select id="findOneUsername" resultType="com.shop.entiy.User">
		select id,username,password,e_mail,phone 
		from shop_user
		where username = #{username}
	</select>
</mapper>