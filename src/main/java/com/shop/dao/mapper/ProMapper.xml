<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.dao.mapper.ProMapper">

	<insert id="createPro">

		insert into
		pro(id,name,price,describe,sort_id)
		values(seq_shop_user.nextval,#{name},#{price},#{describe},#{sortId})
	</insert>
</mapper>